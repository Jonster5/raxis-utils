{
	"name": "@raxis/option",
	"version": "0.0.0",
	"description": "Simple and fast Option type",
	"author": "Jonster5",
	"scripts": {
		"build": "tsup",
		"test": "vitest run"
	},
	"publishConfig": {
		"access": "public"
	},
	"main": "dist/index.js",
	"module": "dist/index.mjs",
	"types": "dist/index.d.ts",
	"files": [
		"dist/*"
	],
	"devDependencies": {
		"@config/ts": "*",
		"@swc/core": "^1.3.102",
		"terser": "^5.26.0",
		"tsup": "^8.0.1",
		"typescript": "^5.3.3",
		"vitest": "1.2.0"
	},
	"tsup": {
		"entry": [
			"src/index.ts"
		],
		"dts": true,
		"clean": true,
		"format": [
			"esm",
			"cjs"
		],
		"minify": true
	}
}
